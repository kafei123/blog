{
    "routes": [
      { "src": "/.*\\.(ico|css|js|png|jpg|svg)$", "headers": { "Cache-Control": "public, max-age=31536000" } },
      {
        "src": "/.*",
        "dest": "/index.html",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      }
    ]
  }